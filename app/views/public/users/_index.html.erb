<table class="table">
  <thead>
    <tr>
      <th>プロフォール画像</th>
      <th>アカウント名</th>
      <th>自己紹介</th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= image_tag(user.get_profile_image) %></td>
        <td><%= user.name %></td>
        <td><%= user.introduction %></td>
      </tr>
    <% end %>
  </tbody>
</table>